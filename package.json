{
  "name": "clickup-mcp",
  "version": "1.0.0",
  "description": "ClickUp Model Context Protocol Server - Production Ready",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "clickup-mcp": "./bin/clickup-mcp"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "start:debug": "node debug-start.js",
    "prestart": "npm run build",
    "railway:build": "npm install && npm run build",
    "test": "npm run build && node test/deployment-test.js",
    "test:quick": "node test/deployment-test.js"
  },
  "keywords": ["mcp", "clickup", "api", "automation", "vinyl-marketing"],
  "author": "Andrew Eades <andrew@vinylmarketing.com>",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
